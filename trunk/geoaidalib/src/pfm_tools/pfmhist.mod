<MODULE name="HistogrammPFM" class="PFM Evaluate"
        cmd="pfmhist @input@ @output@ @min@ @max@ @step@ @x1@ @y1@ @x2@ @y2@">
  <INPUT name=input>
  <OUTPUT name=output>
  <TEXT name=min>
  <TEXT name=max>
  <TEXT name=step>
  <TEXT name=x1>
  <TEXT name=y1>
  <TEXT name=x2>
  <TEXT name=y2>
</MODULE>
