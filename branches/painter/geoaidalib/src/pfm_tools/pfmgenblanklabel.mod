<MODULE class="PFM Evaluate" name=GenBlankLabelImage
        tip="generate a blank label image with the given id"
	cmd="pfmgenblanklabel @input@ @output@ @id@ ">
  <INPUT name=input label="Input: " tip="Input File">
  <OUTPUT name=output depend=input label="Output: " tip="Output File">
  <TEXT name=id>
</MODULE>
	
